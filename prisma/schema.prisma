// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Car {
  id        String    @default(uuid())
  name      String    @unique
  longitude Float
  latitude  Float
  timestamp DateTime
  updatedAt DateTime?

  @@id([id, name])
}
